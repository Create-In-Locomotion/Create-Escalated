{
  "schemaVersion": 1,
  "id": "escalated",
  "version": "${version}",
  "name": "Create: Escalated",
  "description": "Bringing you to higher places in your Create mod builds",
  "authors": [
    "rbasamoyai"
  ],
  "contact": {
    "homepage": "https://github.com/Create-In-Locomotion/Create-Escalated",
    "sources": "https://github.com/Create-In-Locomotion/Create-Escalated/issues"
  },
  "license": "MIT",
  "icon": "assets/escalated/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "rbasamoyai.escalated.fabric.CreateEscalatedFabric"
    ]
  },
  "mixins": [
    "escalated.mixins.json",
    "escalated-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}"
  }
}